{
    "devDependencies": {
        "eslint": "^9.12.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx,json,css,md}": "git add"
    },
    "scripts": {
        "prepare": "husky",
        "lint": "eslint .",
        "format": "prettier --write ."
    }
}
